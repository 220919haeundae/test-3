package com.kh.practice1;

import java.util.Scanner;

public class OperationPractice2 {

	public static void main(String[] args) {
		double x;
		double y;
		
		Scanner sc = new Scanner(System.in);
		System.out.print("가로: ");
		x = sc.nextDouble();
		System.out.print("세로: ");
		y = sc.nextDouble();
	}
}
