package com.kh.practice.set;

public class Person {
	private String name;
	private int age;
	private double height;
	
	public int hashCode() {
		return name.hashCode() + age;
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof Person) {
			return this.name.equals(((Person)obj).name) && (this.age == ((Person)obj).age);
		} else return false;
	}

}
